---
import Layout from "./BaseLayout.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import "../styles/global.css";
export interface Props {
  title: String;
}

const { title } = Astro.props;
---

<Layout {title}>
  <slot name="header">
    <Header />
  </slot>
  <main>
    <slot />
  </main>
  <slot name="footer">
    <Footer />
  </slot>
</Layout>
